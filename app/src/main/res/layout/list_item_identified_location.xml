<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/list_item_identified_location"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="5dp"
    android:paddingTop="5dp">


    <TextView
        android:id="@+id/wifi_infoSSDI"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textStyle="bold"
        android:layout_alignParentTop="true" />

    <TextView
        android:id="@+id/wifi_infoMACtext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MAC: "
        android:layout_below="@+id/wifi_infoSSDI"
        />

    <TextView
        android:id="@+id/wifi_infoMAC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/wifi_infoMACtext"
        android:layout_below="@+id/wifi_infoSSDI"
        />

    <TextView
        android:id="@+id/wifi_infoRSSI"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/wifi_infoMAC" />

    <ImageView
        android:id="@+id/compare_Status_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:layout_alignParentRight="true"
        />

</RelativeLayout>